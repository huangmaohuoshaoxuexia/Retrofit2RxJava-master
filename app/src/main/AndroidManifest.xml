<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.github.ws.retrofitview">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".SimpleGetActivity"></activity>
        <activity android:name=".GetActivity"></activity>
        <activity android:name=".PostActivity"></activity>
        <activity android:name=".UploadSimpleFileActivity"></activity>
        <activity android:name=".UploadMultiFileActivity"></activity>
        <activity android:name=".DownFileActivity"></activity>
        <activity android:name=".RxJavaActivity"></activity>
    </application>

</manifest>